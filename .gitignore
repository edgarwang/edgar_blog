# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore deploy conf file
/config/deploy.rb

# Ignore newrelic config file
config/newrelic.yml

# Ignore uploads dir
public/uploads

# Ignore coverage dir
coverage/
